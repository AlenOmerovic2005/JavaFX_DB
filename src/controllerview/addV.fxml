<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox prefHeight="337.0" prefWidth="512.0" spacing="10.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllerview.addC">
    <children>
        <Label text="ID" />
        <TextField fx:id="idField" prefHeight="31.0" prefWidth="275.0" />
        <Label text="Name" />
        <TextField fx:id="nameField" />
        <Label text="Wohnort" />
        <TextField fx:id="wohnortField" alignment="CENTER" />
      <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0" spacing="20.0">
         <children>
              <Button onAction="#handleOK" text="OK" />
              <Button onAction="#handleCancel" text="Cancel" />
         </children>
         <VBox.margin>
            <Insets />
         </VBox.margin>
         <padding>
            <Insets left="20.0" />
         </padding>
      </HBox>
    </children>
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>
</VBox>
